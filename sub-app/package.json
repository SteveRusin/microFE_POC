{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "sub-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && npm run serve",
    "serve": " http-server ./dist/bundles -p 8081 --cors=http://localhost:8080/",
    "build": "ng build --extra-webpack-config webpack.externals.js --output-hashing none",
    "build:once": "ng build --extra-webpack-config webpack.externals.js --prod --single-bundle --output-hashing none",
    "test": "ng test",
    "watch": "nodemon --watch . -e ts,html,scss --exec \"npm run build:plugin\"",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "debug": "nodemon --inspect-brk ./bin/ng build plugins --prod --modulePath=./app/app.module#SubAppModule --pluginName=sub-app --outputPath=./dist/bundles",
    "build:plugin": "ng build plugins --prod --modulePath=./app/app.module#SubAppModule --pluginName=sub-app --outputPath=./dist/bundles",
    "build:externals": "ng build --extra-webpack-config webpack.externals.js --prod --single-bundle",
    "build:sub-app:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project sub-app --single-bundle"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "src/main.ts"
    ]
  },
  "ngPackage": {
    "lib": {
      "entryFile": "./src/main.ts"
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "bundle-loader": "^0.5.6",
    "ngx-build-plus": "^9.0.2",
    "nodemon": "^2.0.2",
    "rxjs": "~6.4.0",
    "shared-app21": "0.0.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular-devkit/build-ng-packagr": "^0.803.23",
    "@angular-devkit/schematics-cli": "^0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-subresource-integrity": "^1.4.0"
  }
}
